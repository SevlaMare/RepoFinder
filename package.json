    "postcss": "~8.5.6",
    "react": "~19.1.1",
    "react-dom": "~19.1.1",
    "react-router-dom": "~7.7.1",
    "tailwindcss": "~4.1.11",
    "uuid": "~11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "~1.54.1",
    "@vitejs/plugin-react": "4.7.0",
    "jsdoc": "^4.0.4",
    "typescript": "5.8.3",
    "typescript-eslint": "~8.38.0",
    "vite": "~7.0.6"
