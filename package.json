    "prettier": "~3.6.2",
    "stylelint": "~16.23.0",
    "stylelint-config-standard": "~39.0.0",
    "stylelint-prettier": "~5.0.3"
  },
  "packageManager": "yarn@1.22.22",
  "devEngines": {
    "node": "~22.17.1",
